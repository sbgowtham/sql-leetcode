
CREATE TABLE Weather (
    id INT PRIMARY KEY,
    recordDate DATE UNIQUE,
    temperature INT
);
INSERT INTO Weather (id, recordDate, temperature) VALUES
(1, '2015-01-01', 10),
(2, '2015-01-02', 25),
(3, '2015-01-03', 20),
(4, '2015-01-04', 30);




select w1.id 
from Weather w1 
join Weather w2 
on datediff(w1.recordDate,w2.recordDate)= 1
where w1.temperature > w2.temperature
